# every misc jar included in lib 
for JAR in $(find $HOME/lib/*  -maxdepth 1 -name "*.jar"); do
  CLASSPATH=$CLASSPATH:$JAR;
done

for JAR in $(find $HOME/lib/*/lib -name "*.jar"); do
  CLASSPATH=$CLASSPATH:$JAR;
done

# export CLASSPATH
export CLASSPATH=$CLASSPATH:/usr/local/Cellar/clojure-contrib/HEAD