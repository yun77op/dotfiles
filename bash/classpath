# CLASSPATH
export CLASSPATH_HOME=$HOME/lib/
# clojure
export CLOJURE_HOME=$CLASSPATH_HOME/clojure
export CLASSPATH=$CLASSPATH:.:classes
export CLASSPATH=$CLASSPATH:$CLOJURE_HOME/clojure-contrib/clojure-contrib.jar
export CLASSPATH=$CLASSPATH:$CLOJURE_HOME/clojure.jar
# lucene
export CLASSPATH=$CLASSPATH:$CLASSPATH_HOME/lucene/lucene-core-2.4.1.jar
# glassfish
export GLASSFISH_HOME=$CLASSPATH_HOME/glassfish
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/j2ee.jar
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/appserv-rt.jar
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/javaee.jar
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/j2ee-svc.jar
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/appserv-ee.jar
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/activation.jar
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/dbschema.jar 
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/appserv-admin.jar 
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/install/applications/jmsra/imqjmx.jar   
export CLASSPATH=$CLASSPATH:$GLASSFISH_HOME/lib/install/applications/jmsra/imqjmsra.jar
# android-sdk
export CLASSPATH=$CLASSPATH:$CLASSPATH_HOME/android-sdk/android.jar